{
	"compilerOptions": {
		"target": "ES2022",
		"module": "Node16",
		"rootDir": "./script",
		"declaration": true,
		"declarationMap": true,
		"outDir": "./lib",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node16",
		"useUnknownInCatchVariables": false
	},
	"include": ["script"],
	"exclude": ["node_modules", "lib"]
}
